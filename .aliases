# Docker shortcut
alias dm='docker-machine'
alias de='eval $(docker-machine env default)'

alias drmc='docker rm $(docker ps --all --quiet) -f'
alias drmi='docker rmi $(docker images --quiet) -f'
alias drmv='docker volume rm `docker volume ls -q -f dangling=true`'

